/**
 * Check that a class can be assigned to a Class variable.
 */
class T {
  test "assign" {
    var c : Class = null;
    c = T;
    T == c && c == T
  }

  test "new" <: noCompile[TYP_NOT_A_TYPE] {
    var c : Class = null;
    c = T;
    val r = T == c && c == T
    // cannot prove that a constructor exists
    delete new c;
    r
  }

  test "new val" {
    val c = T;
    val r = T == c && c == T
    // type vals are CT-evaluated
    delete new c;
    r
  }
}
