/**
 * Test explicit override explicit no this and type def realizations
 */
interface I {
  class def fun() : int = 1
}
interface J <: I {
}

class T <: J {

  // the explicit and implicit method slots should count as the same method slot
  override[I.fun]
  type def fun() : int = 3

  test "use" {
    var x = new T;
    {
      3 == x.fun()
    } && {
      delete x;
      true
    }
  }
}
