/**
 * Test explicit override explicit no this and type def realizations
 */
interface I {
  class def fun(x : int) : int = x
}
interface J <: I {
}

class T <: J {

  override[I.fun]
  type def fun(x : int) : int = 3*x

  test "use" {
    var x = new T;
    {
      9 == x.fun(3)
    } && {
      delete x;
      true
    }
  }
}
