/**
 * Check super destructor call from a class
 */
class S {
  type var x : bool
  delete {x = true}
}

class T <: S {
  delete {
    S.delete
  }

  test "use S" {
    S.x = false;
    var s = new S
    !S.x && {
      delete s
      S.x
    }
  }

  test "use T" {
    S.x = false;
    var s = new T
    !S.x && {
      delete s
      S.x
    }
  }

  test "invalid" <: noCompile {
    S.delete
  }
}
