/**
 * Check that a type variable with base type Class can use Object members
 */
type Box[T : Type] {
  var x : T;
  new (x := x : T);

  delete {
    // should not be provable
    delete x;
  }

  test "use" {
    var b = new Box[String](null);
    delete b
  }

  test "use eq" {
    var b = new Box[String](null);
    var r = b.x == b.x
    delete b
    return r
  }
}
