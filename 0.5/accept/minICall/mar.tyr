/**
 * Test interface calls
 */
interface WithFun {
  // @note interfaces should require neither new nor delete

  def fun : bool = true
}

class Test <: WithFun {
  new; delete;

  test "new" {
    delete new Test
  }

  test "new interface" <: noCompile {
    delete new WithFun
  }

  test "call" {
    var x = new Test
    val r = x.fun()
    delete x
    r
  }

  test "icall" {
    var x : WithFun = new Test
    val r = x.fun()
    delete x
    r
  }
}
