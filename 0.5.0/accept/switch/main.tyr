/**
 * Test switch and guard type inference
 */
type Switch {

  test "var" {
    var x = 3
    switch x {
      if 0 false
      if 7 false
      else true
    }
  }

  test "int / long" <: noCompile[TYP_MISMATCH] {
    var x = 3
    switch x {
      if 0 false
      if 7L false
      else true
    }
  }
}
