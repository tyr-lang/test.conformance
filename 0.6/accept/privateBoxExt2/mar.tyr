/**
 * Ensure that usages of namespaces are correctly translated by substitutions
 *
 * @note try an existing type parameter
 */
type BT {
  with pb.a.Box;

  test "instantiate " {
    val x = new Box[long](0L)
  }

  test "use" <: noCompile {
    val x = new Box[long](0L)
    // should not compile
    0L == x.x
  }
}
