/**
 * Class interface delete cannot call delete
 */
interface R;

class B <: R {
  type var called = 0
  delete {
    ++called
  }
}

interface I {
  type var called = false
  delete {
    // not allowed, called by whichever class inherits I
    B.delete
  }
}

interface J <: I {
  // nothing, just a marker, no implicit members
}

class E <: B, J {
  test "fail"{
    false
  }
}