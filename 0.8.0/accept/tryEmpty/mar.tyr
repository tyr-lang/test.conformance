/**
 * try should work well with empty blocks
 *
 * @note  it would be fine if these tests would create warnings
 */
class T {
  test "catch" {
    try {
    } catch e {
      if Exception {}
      else {}
    }
  }

  test "catch else" {
    try {
    } catch e {
      else {}
    }
  }

  test "finally" {
    try {
    } finally {
    }
  }
}
