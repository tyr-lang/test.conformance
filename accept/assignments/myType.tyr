type MyType {
  test "this assignable" {
    var this = new MyType();
    var that = this;
    assert (this == that);

    this = new MyType();
    assert (this != that);
    assert (this != null);
    assert (null != that);

    this = null;
    assert (this != that);
    assert (this == null);
    assert (null != that);

    that = null;
    assert (this == that);
    assert (this == null);
    assert (null == that);
  }
}
