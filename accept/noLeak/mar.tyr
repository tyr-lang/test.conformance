/**
 * @note tests correctness of .type for ints
 */
class T {
  test "basic" {
    val x = new T
    delete x
  }
  test "basic var" {
    var x = new T
    null != x && ({
      delete x
      null == x
    })
  }
  test "basic literal" {
    // TODO this test should not work before 0.7
    IntegerLiteral[32] == 7.type
  }
}
